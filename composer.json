{
  "name": "wordpresspluginpro/wpp-plugin-skeleton",
  "type": "project",
  "author": "Dan Alvidrez <info@wordpresspluginpro.com>",
  "require": {
    "bayareawebpro/illumine-framework":"dev-master"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "IlluminePlugin1\\": "app"
    }
  },
  "post-install-cmd": [
    "composer dump-autoload -o"
  ],
  "post-update-cmd": [
    "composer dump-autoload -o"
  ]
}
